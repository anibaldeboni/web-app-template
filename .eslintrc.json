{
  "extends": [
    "airbnb-typescript/base",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
        "@typescript-eslint",
        "import"
    ],
  "env": {
    "jest": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "@typescript-eslint/no-empty-interface": 0
  }
}